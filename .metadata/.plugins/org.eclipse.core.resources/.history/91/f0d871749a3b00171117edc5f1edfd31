private static Volt getvolt(SocketAdaptor adaptor,int i){
	switch (i){
	case 3:return adaptor.get3Volt();
	case 12:return adaptor.get12Volt();
	case 120:return adaptor.get120Volt();
	default: return adaptor.get120Volt();
	}
}
